{
    "compilerOptions": {
        "target": "ES2020",
        "module": "es6",
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "strict": true,
        "noUnusedLocals": true,
        "strictNullChecks": true
    },
    "include": ["./src/*"]
}
